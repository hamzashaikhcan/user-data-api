# Environment (development, test, production)
NODE_ENV=development

# Server
PORT=3000

# Cache settings
CACHE_MAX_SIZE=100
CACHE_TTL_SECONDS=60

# Rate limiting
RATE_LIMIT_MAX=10
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_BURST_MAX=5
RATE_LIMIT_BURST_WINDOW_MS=10000

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
