{
    "name": "user-data-api",
    "version": "1.0.0",
    "description": "Express.js API with advanced caching, rate limiting, and asynchronous processing",
    "main": "dist/server.js",
    "scripts": {
      "build": "tsc",
      "start": "node dist/server.js",
      "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix"
    },
    "keywords": [
      "express",
      "typescript",
      "cache",
      "rate-limiting",
      "async"
    ],
    "author": "Hamza Zahid",
    "license": "MIT",
    "dependencies": {
      "bull": "^4.12.0",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-async-handler": "^1.2.0",
      "express-prom-bundle": "^6.6.0",
      "express-rate-limit": "^7.1.4",
      "helmet": "^7.1.0",
      "lru-cache": "^10.0.1",
      "morgan": "^1.10.0",
      "prom-client": "^14.2.0",
      "response-time": "^2.3.2",
      "winston": "^3.11.0"
    },
    "devDependencies": {
      "@types/bull": "^4.10.0",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.10",
      "@types/morgan": "^1.9.9",
      "@types/node": "^20.10.0",
      "@types/response-time": "^2.3.8",
      "@types/supertest": "^2.0.16",
      "jest": "^29.7.0",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.3.2"
    }
  }